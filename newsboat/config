#auto-reload yes

bind-key h quit
bind-key j down
bind-key k up
bind-key l open

bind-key J next-unread
bind-key K prev-unread

bind-key U home
bind-key D end

bind-key u pageup
bind-key d pagedown

browser $TERMINAL_BROWSER

color listnormal default default
color listfocus cyan black standout
color listnormal_unread blue default
color listfocus_unread blue black standout
color info black cyan
color article default default

