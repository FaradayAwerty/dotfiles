#auto-reload yes

bind-key h quit
bind-key j down
bind-key k up
bind-key l open

bind-key J next-unread
bind-key K prev-unread

bind-key g home
bind-key G end

bind-key u pageup
bind-key d pagedown

browser $TERMINAL_BROWSER

color listnormal default default
color listfocus blue black standout
color listnormal_unread blue default
color listfocus_unread cyan black standout
color info blue black
color article default default

