#auto-reload yes

bind-key l open
bind-key h quit
bind-key j down
bind-key k up

bind-key n next-unread
bind-key p prev-unread

bind-key K home
bind-key J end

bind-key u pageup
bind-key d pagedown

browser $TERMINAL_BROWSER

color listnormal default default
color listfocus blue default standout
color listnormal_unread green default
color listfocus_unread green black standout
color info default blue
color article default default

