# --- movement --- #
keymap	h	MOVE_LEFT
keymap	j	MOVE_DOWN
keymap	k	MOVE_UP
keymap	l	MOVE_RIGHT

keymap	w	NEXT_WORD
keymap	W	NEXT_WORD
keymap	b	PREV_WORD
keymap	B	PREV_WORD

keymap	J	NEXT_LINK
keymap	K	PREV_LINK

keymap	0	LINE_BEGIN
keymap	^	LINE_BEGIN
keymap	$	LINE_END

keymap	G	END
keymap	gg	BEGIN

keymap	C-u	DOWN 12
keymap	C-d	UP 12

keymap	C-f	NEXT_PAGE
keymap	C-b	PREV_PAGE

keymap	C-y	DOWN
keymap	C-e	UP

keymap	/	SEARCH
keymap	n	SEARCH_NEXT
keymap	p	SEARCH_PREV


# --- browsing --- #
keymap	r	RELOAD
keymap	o	GOTO
keymap	Q	QUIT

keymap	z	BACK

keymap	H	HISTORY
keymap	D	DOWNLOAD_LIST

keymap	f	LINK_BEGIN
keymap	F	MOVE_LIST_MENU

keymap	s	GOTO 'https://duckduckgo.com/html'
keymap	S	GOTO 'https://duckduckgo.com/'


# --- actions on urls --- #
keymap	u	EXTERN_LINK 'tmux new-window -v -p 20 exurl'
keymap	U	EXTERN 'tmux new-window -v -p 20 exurl'

keymap	v	PEEK_LINK
keymap	V	PEEK


# --- images --- #
keymap	I	VIEW_IMAGE
keymap	C-i	SAVE_IMAGE
keymap	i	PEEK_IMG


# --- bookmarks --- #
keymap	m	BOOKMARK
keymap	M	ADD_BOOKMARK


# --- tabs --- #
keymap	tt	TAB_LINK
keymap	tn	NEW_TAB
keymap	th	PREV_TAB
keymap	tl	NEXT_TAB
keymap	q	CLOSE_TAB


# --- misc --- #
keymap	E	EDIT_SCREEN
keymap	O	OPTIONS


# --- unbind --- #

